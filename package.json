{
  "name": "discordjs-bot",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:phill-dev/discordjs-bot.git",
  "author": "<p.s.meyer@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "deploy:guild": "ts-node -r tsconfig-paths/register ./src/deploy-slash-commands.ts",
    "run:dev": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "run:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "build": "tsc -p tsconfig.prod.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "discord-api-types": "^0.35.0",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "dotenv-safe": "^8.2.0"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
